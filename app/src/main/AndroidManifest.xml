<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ne.subgrupo_catorce.proyecto_final">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="NE - PROYECTO FINAL"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NESubGrupo14ProyectoFinal">

        <activity
            android:name="ne.subgrupo_catorce.proyecto_final.DashboardActivity"
            android:exported="true"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.NESubGrupo14ProyectoFinal.NoActionBar" />
        <activity
            android:name="ne.subgrupo_catorce.proyecto_final.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.NESubGrupo14ProyectoFinal.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>